{
  "name": "guess-game",
  "version": "1.0.0",
  "description": "Simple Guess Game, based on React",
  "main": "src/index.js",
  "scripts": {
    "babel": "babel client -d _tmp/babel",
    "babel-watch": "babel client -d _tmp/babel -w",
    "browserify": "browserifyinc -e _tmp/babel/index.js -o build/index.js --cachefile _tmp/browserify/cache.json --debug",
    "static": "node folders.js && cp -R static/*.* build/",
    "build": "npm run static && npm run babel && npm run browserify",
    "build-watch": "npm run static && npm run babel-watch",
    "rebuild": "npm run browserify",
    "server": "node server.js",
    "test": "mocha --require babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/62mkv/guess-game.git"
  },
  "keywords": [
    "game",
    "react"
  ],
  "author": "62mkv",
  "license": "ISC",
  "dependencies": {
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "hapi": "^15.2.0",
    "inert": "^4.0.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-cli": "^6.16.0",
    "babel-register": "^6.18.0",
    "browserify": "^13.1.1",
    "browserify-incremental": "^3.1.1",
    "mocha": "^3.2.0",
    "redux-devtools": "^3.3.1",
    "shelljs": "^0.7.5"
  }
}
